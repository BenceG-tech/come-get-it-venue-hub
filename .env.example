
# Supabase (Frontend)
VITE_SUPABASE_URL="https://YOUR_PROJECT_REF.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="YOUR_ANON_KEY"

# Environment configuration
VITE_ENVIRONMENT=development  # or staging, production
VITE_USE_SUPABASE=false       # Default provider preference
VITE_FORCE_SUPABASE=false     # Force Supabase regardless of environment

# Provider switching (Development/Staging only):
# - URL: ?provider=supabase or ?provider=mock
# - localStorage: localStorage.setItem('provider', 'supabase') or 'mock'
# Note: In production, provider is always locked to Supabase
